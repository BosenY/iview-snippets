{
    "ivew row": {
        "prefix": "iview-Row",
        "body": [
            "<Row>\n\t$1\n</Row>"
        ],
        "description": "iview row"
    },
    "ivew col": {
        "prefix": "iview-Col",
        "body": [
            "<Col span=\"$1\">\n\t$2\n</Col>"
        ],
        "description": "iview span"
    },
    "ivew button": {
        "prefix": "iview-Button",
        "body": [
            "<Button type=\"${1:primary}\">$2</Button>"
        ],
        "description": "iview button"
    },
    "ivew input": {
        "prefix": "iview-Input",
        "body": [
            "<Input v-model=\"${1}\" placeholder=\"${2}\"></Input>"
        ],
        "description": "iview input"
    },
    "ivew input number": {
        "prefix": "iview-Inputnumber",
        "body": [
            "<Input-number :max=\"${1:10}\" :min=\"${2:1}\" v-model=\"${3:model}\"></Input-number>"
        ],
        "description": "iview input number"
    },
    "ivew table": {
        "prefix": "iview-Table",
        "body": [
            "<Table :columns=\"${1:column}\" :data=\"${2:data}\"></Table>"
        ],
        "description": "iview table"
    },
    "ivew select": {
        "prefix": "iview-Select",
        "body": [
            "<Select v-model=\"${1:model}\">\n\t<Option v-for=\"${2:item} in ${3:list}\" :value=\"${4:value}\" :key=\"item\">${5}</Option>\n</Select>"
        ],
        "description": "iview select"
    },
    "ivew datetime picker": {
        "prefix": "iview-Datepicker",
        "body": [
            "<Date-picker type=\"datetime\" format=\"yyyy/MM/dd HH:mm\" placeholder=\"选择日期和时间\"></Date-picker>"
        ],
        "description": "iview datetime picker"
    },
    "ivew upload": {
        "prefix": "iview-Upload",
        "body": [
            "<Upload action=\"${1}\">\n\t<Button type=\"ghost\" icon=\"ios-cloud-upload-outline\">上传文件</Button>\n</Upload>"
        ],
        "description": "iview upload"
    },
    "ivew message": {
        "prefix": "iview-Message",
        "body": [
           "this.$$Message.info('${1}');"
        ],
        "description": "iview message"
    },
    "iview notice": {
        "prefix": "iview-Notice",
        "body": [
            "this.$$Notice.open({",
                "\ttitle: '',",
                "\tdesc: ''",
            "});"
        ],
        "description": "iview notice"
    },
    "iview form": {
        "prefix": "iview-Form",
        "body": [
            "<Form ref=\"${1}\" :model=\"${2}\" :rules=\"${3}\" inline>",
            "\t<FormItem label=\"${4}\">",
            "\t\t${5}",
            "\t</FormItem>",
            "</Form>"
        ],
        "description": "iview form"
    },
    "iview radio": {
        "prefix": "iview-Radio",
        "body": [
            "<Radio v-model=\"${1}\">${2}</Radio>"
        ],
        "description": "iview radio"
    },
    "iview check-box": {
        "prefix": "iview-CheckBox",
        "body": [
            "<Checkbox v-model=\"${1}\">${2}</Checkbox>"
        ],
        "description": "iview check-box"
    }

}