{
    "ivew row": {
        "prefix": "iRow",
        "body": [
            "<Row>\n\t$1\n</Row>"
        ],
        "description": "iview row"
    },
    "ivew col": {
        "prefix": "iCol",
        "body": [
            "<Col span=\"$1\">\n\t$2\n</Col>"
        ],
        "description": "iview span"
    },
    "ivew icon": {
        "prefix": "iIcon",
        "body": [
            "<Icon type=\"$1\" />"
        ],
        "description": "iview icon"
    },
    "ivew card": {
        "prefix": "iCard",
        "body": [
            "<Card>",
            "\t<p slot=\"title\">$1</p>",
            "</Card>"
        ],
        "description": "iview card"
    },
    "ivew button": {
        "prefix": "iButton",
        "body": [
            "<Button type=\"${1:primary}\">$2</Button>"
        ],
        "description": "iview button"
    },
    "ivew buttongroup": {
        "prefix": "iButtonGroup",
        "body": [
            "<ButtonGroup><Button>Cancel</Button>\n<Button type=\"${1:primary}\">$2</Button></ButtonGroup>"
        ],
        "description": "iview buttongroup"
    },
    "ivew input": {
        "prefix": "iInput",
        "body": [
            "<Input v-model=\"${1}\" placeholder=\"${2}\"></Input>"
        ],
        "description": "iview input"
    },
    "ivew input number": {
        "prefix": "iInputnumber",
        "body": [
            "<Input-number :max=\"${1:10}\" :min=\"${2:1}\" v-model=\"${3:model}\"></Input-number>"
        ],
        "description": "iview input number"
    },
    "ivew table": {
        "prefix": "iTable",
        "body": [
            "<Table :columns=\"${1:column}\" :data=\"${2:data}\"></Table>"
        ],
        "description": "iview table"
    },
    "ivew select": {
        "prefix": "iSelect",
        "body": [
            "<Select v-model=\"${1:model}\">\n\t<Option v-for=\"${2:item} in ${3:list}\" :value=\"${4:value}\" :key=\"item\">${5}</Option>\n</Select>"
        ],
        "description": "iview select"
    },
    "ivew datetime picker": {
        "prefix": "iDatepicker",
        "body": [
            "<Date-picker type=\"datetime\" format=\"yyyy/MM/dd HH:mm\" placeholder=\"选择日期和时间\"></Date-picker>"
        ],
        "description": "iview datetime picker"
    },
    "ivew upload": {
        "prefix": "iUpload",
        "body": [
            "<Upload action=\"${1}\">\n\t<Button type=\"ghost\" icon=\"ios-cloud-upload-outline\">上传文件</Button>\n</Upload>"
        ],
        "description": "iview upload"
    },
    "ivew message": {
        "prefix": "iMessage",
        "body": [
           "this.$$Message.info('${1}');"
        ],
        "description": "iview message"
    },
    "iview notice": {
        "prefix": "iNotice",
        "body": [
            "this.$$Notice.open({",
                "\ttitle: '',",
                "\tdesc: ''",
            "});"
        ],
        "description": "iview notice"
    },
    "iview form": {
        "prefix": "iForm",
        "body": [
            "<Form ref=\"${1}\" :model=\"${2}\" :rules=\"${3}\" inline>",
            "\t<FormItem label=\"${4}\">",
            "\t\t${5}",
            "\t</FormItem>",
            "</Form>"
        ],
        "description": "iview form"
    },
    "iview model": {
        "prefix": "iModal",
        "body": [
            "<Modal",
            "\tv-model=\"${1}\"",
            "\ttitle=\"${2}\"",
            "\t>",
            "\t${3}",
            "</Modal>"
        ],
        "description": "iview modal"
    },
    "iview radio": {
        "prefix": "iRadio",
        "body": [
            "<Radio v-model=\"${1}\">${2}</Radio>"
        ],
        "description": "iview radio"
    },
    "iview radiogroup": {
        "prefix": "iRadioGroup",
        "body": [
            "<RadioGroup v-model=\"${1}\">",
            "\t<Radio label=\"${2}\"></Radio>",
            "\t<Radio label=\"${3}\"></Radio>",
            "</RadioGroup>"
        ],
        "description": "iview radiogroup"
    },
    "iview check-box": {
        "prefix": "iCheckBox",
        "body": [
            "<Checkbox v-model=\"${1}\">${2}</Checkbox>"
        ],
        "description": "iview check-box"
    },
    "iview checkboxgroup": {
        "prefix": "iCheckboxGroup",
        "body": [
            "<CheckboxGroup v-model=\"${1}\">",
            "\t<Checkbox label=\"${2}\"></Checkbox>",
            "\t<Checkbox label=\"${3}\"></Checkbox>",
            "</CheckboxGroup>"
        ],
        "description": "iview checkboxgroup"
    }

}
